@PROPS@ fontColor=black ; autoExecute=true ; fontSize=1.00em ; sortBy= ; color=cyan ; playerEditable=false ; applyToSelected=false ; group=0 Coinpurse ; tooltip= ; minWidth=140
[H: cpData = getProperty("Coinpurse")]
