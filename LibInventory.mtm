	[H: armdeets = json.get(ArmorDetails, item)]
	[H: useinfo = json.get(armdeets, "UseInfo")]
	[H: empty = json.isEmpty(useinfo)]
	[H, if(empty == 1): shortlist = listAppend(shortlist, item)]
}]

